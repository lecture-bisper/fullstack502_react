<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bitc.full502.boardapiserver.mapper.BoardMapper">

  <select id="selectBoardList" resultType="bitc.full502.boardapiserver.dto.BoardDTO">
    SELECT board_idx, title, contents, create_id, create_date, update_id, update_date
    FROM t_board
    ORDER BY board_idx DESC
  </select>

</mapper>